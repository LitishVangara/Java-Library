package autoUnBoxingPrograms1;

public class AutoUnBoxingExample 
{
	public static void main(String[] args) 
	{
		int a = 65;
		Integer a1 = a;
		System.out.println(a1.toString());
		boolean a2 = true;
		Boolean a3 = a2;
		System.out.println(a3);
		//double a4 = 10.8;
		Double a5 = 0.2;
		Double a6 = a5;
		System.out.println(a6);
	}
}
