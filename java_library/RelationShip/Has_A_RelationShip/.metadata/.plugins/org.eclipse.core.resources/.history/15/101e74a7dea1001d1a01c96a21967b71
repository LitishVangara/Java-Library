package earlyBinding4;

public class Watches {
	public int cells;
	public Watches() {}
	public Watches(int cells)
	{
		this.cells = cells;
	}
	Cells c1 = new Cells();
	public void use()
	{
		System.out.println(this.cells+" Cells are used to use Watch.");
		System.out.println("You can use the Watch now.");
	}
}
