package earlyInstanstiationPrograms1;

public class EarlySingletonExample 
{
	public static void main(String[] args) 
	{
		EarlySingleton obj = EarlySingleton.getObject();
		System.out.println(obj.status);
		EarlySingleton obj1 = EarlySingleton.getObject();
		System.out.println(obj1.status);
		System.out.println(obj == obj1);
	}
}
