package pojoClassPrograms1;

public class Student 
{
	private String name;
	private int id;
	private int degree;
	public Student () {}
	public Student(String name, int id, int degree) 
	{
		this.name = name;
		this.id = id;
		this.degree = degree;
	}
	public String getName() 
	{
		return name;
	}
	public void setName(String name) 
	{
		this.name = name;
	}
	public int getId() 
	{
		return id;
	}
	public void setId(int id) 
	{
		this.id = id;
	}
	public int getDegree() 
	{
		return degree;
	}
	public void setDegree(int degree) 
	{
		this.degree = degree;
	}
	
}
